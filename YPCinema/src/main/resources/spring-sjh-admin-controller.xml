<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans
	    http://www.springframework.org/schema/beans/spring-beans.xsd">
	<!-- Controller -->
	<bean class="controller.admin.JicmuController" />
	<bean class="controller.admin.TheaterController" />
	<bean class="controller.admin.CouponController" />
	<bean class="controller.admin.CouponIssueController" />

	
	<!-- Service -->
	<bean id="theaterAddService" class="service.admin.TheaterAddService" />
	<bean id="jicmuListService" class="service.admin.JicmuListService" />
	<bean id="couponAddService" class="service.admin.CouponAddService" />
	<bean id="couponIssueService" class="service.admin.CouponIssueService" />
	
	<!-- Repository -->	
	<bean id="theaterAddRepository" class="repository.admin.TheaterAddRepository" />
	<bean id="jicmuRepository" class="repository.admin.JicmuRepository" />
	<bean id="couponRepository" class="repository.admin.CouponRepository" />
	<bean id="couponIssueRepository" class="repository.admin.CouponIssueRepository" />
	
	<!-- Cheyong Controller -->
	<bean class="controller.cheyong.CheyongController" />
	<bean class="controller.cheyong.CheyongAddController" />
	<bean class="controller.cheyong.CheyongApplyController" />
	
	<!-- Cheyong Service -->
	<bean id="cheyongService" class="service.cheyong.CheyongService" />
	<bean id="cheyongAddService" class="service.cheyong.CheyongAddService"/>
	<bean id="cheyongApplyService" class="service.cheyong.CheyongApplyService" />
	
	<!-- Cheyong Repository -->
	<bean id="cheyongRepository" class="repository.cheyong.CheyongRepository" />
</beans>